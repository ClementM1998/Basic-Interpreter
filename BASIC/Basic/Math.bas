10 PRINT "PROGRAM MATH"
20 PRINT "ENTER NUMBER A"
30 INPUT A
40 PRINT "ENTER NUMBER B"
50 INPUT B
60 PRINT "CHOOSE OPERATOR 1(+) 2(-) 3(*) 4(/) 5(^)"
70 INPUT OP
80 IF OP=1 THEN GOSUB 200
90 IF OP=2 THEN GOSUB 220
100 IF OP=3 THEN GOSUB 240
110 IF OP=4 THEN GOSUB 260
120 END
200 PRINT A + B
210 RETURN
220 PRINT A - B
230 RETURN
240 PRINT A * B
250 RETURN
260 PRINT A / B
270 RETURN

